/* pages/address/address.wxss */
.main{
  background:#f6f6f6;
  width:100%;min-height:1210rpx;
  margin: 0;padding:0;
  overflow-y: hidden;
}
.addList{
  width:95%;
  margin:0 auto;
  background: #fff;
  border-radius: 5px;
  margin-top: 50rpx;
}
.addList>view{
text-align: center;
}
.addList>view>text{
  display: inline-block;
  margin:20rpx;
}
.btn{
  display:inline-block;
  width:49.8%;
  height:100rpx;
  text-align: center;
  line-height: 100rpx;
  border-top:1px solid #f6f6f6;
}
.dim{
  width:100%;
  height:1500rpx;
  position: fixed;
  top:0;left:0;
  z-index: 100;
  background:rgba(0,0,0,0.6);
}
.dim>view{
  background: #fff;
}